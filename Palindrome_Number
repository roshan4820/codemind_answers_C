#include<stdio.h>
int main(){
    int n,temp,rem,i,a,rev;
    scanf("%d",&a);
    for(i=0;i<a;i++){
        scanf("%d",&n);
        temp=n;
        rev=0;
        while(n!=0){
            rem=n%10;
            rev=rev*10+rem;
            n=n/10;
        }
       if(rev==temp){
          printf("True\n");
        }
        else{
          printf("False\n");
        } 
     }
     return 0;
}
